<%- include("partials/header") -%>
    <h1>Home</h1>
    <p><%= startingContent %></p>
    <% posts.forEach((post) => { %>
        <h2><%= post.title %></h1>
        <p>
            <%= post.content.substring(0, 100) + "..."%>
            <a class="post-links" href="/posts/<%= post._id %>">Read More</a>
        </p>
    <% }); %>
<%- include("partials/footer") -%>